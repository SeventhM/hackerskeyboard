<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="10%p"
    app:horizontalGap="0px"
    app:verticalGap="@dimen/key_bottom_gap">
    <Row app:rowEdgeFlags="top">
        <Key app:keyLabel=";" app:popupKeyboard="@xml/kbd_popup_template"
             app:popupCharacters=";:&#301;&#308;&#300;&#306;&#304;&#302;&#313;&#314;"
             app:keyEdgeFlags="left"/>
        <Key app:keyLabel="ς" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ςΣσ"/>
        <Key app:keyLabel="ε" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="εΕέΈ"/>
        <Key app:keyLabel="ρ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ρΡ"/>
        <Key app:keyLabel="τ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="τΤ"/>
        <Key app:keyLabel="υ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="υΥύΎϋΫΰ"/>
        <Key app:keyLabel="θ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="θΘ"/>
        <Key app:keyLabel="ι" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ιΙίΊϊΪΐ"/>
        <Key app:keyLabel="ο" app:popupKeyboard="@xml/kbd_popup_narrow_template" app:popupCharacters="οΟόΌ"/>
        <Key app:keyLabel="π" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="πΠ" app:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key app:keyLabel="α" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="αΑάΆ" app:horizontalGap="5%p" app:keyEdgeFlags="left"/>
        <Key app:keyLabel="σ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="σΣς"/>
        <Key app:keyLabel="δ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="δΔ"/>
        <Key app:keyLabel="φ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="φΦ"/>
        <Key app:keyLabel="γ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="γΓ"/>
        <Key app:keyLabel="η" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ηΗήΉ"/>
        <Key app:keyLabel="ξ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ξΞ"/>
        <Key app:keyLabel="κ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="κΚ"/>
        <Key app:keyLabel="λ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="λΛ" app:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key app:codes="@integer/key_shift" app:keyIcon="@drawable/sym_keyboard_shift" app:iconPreview="@drawable/sym_keyboard_feedback_shift" app:keyWidth="15%p" app:isModifier="true" app:isSticky="true" app:keyEdgeFlags="left"/>
        <Key app:keyLabel="ζ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ζΖ"/>
        <Key app:keyLabel="χ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="χΧ"/>
        <Key app:keyLabel="ψ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ψΨ"/>
        <Key app:keyLabel="ω" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="ωΩώΏ"/>
        <Key app:keyLabel="β" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="βΒ"/>
        <Key app:keyLabel="ν" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="νΝ"/>
        <Key app:keyLabel="μ" app:popupKeyboard="@xml/kbd_popup_template" app:popupCharacters="μΜ"/>
        <Key app:codes="@integer/key_delete" app:keyIcon="@drawable/sym_keyboard_delete" app:iconPreview="@drawable/sym_keyboard_feedback_delete" app:keyWidth="15%p" app:isModifier="true" app:isRepeatable="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_normal" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="40%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_url" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="40%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_email" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="40%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_im" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="40%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:keyLabel=":-)" app:keyOutputText=":-) " app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_smileys" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_webentry" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:codes="@integer/key_tab" app:keyIcon="@drawable/sym_keyboard_tab" app:iconPreview="@drawable/sym_keyboard_feedback_tab" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="20%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_normal_with_settings_key" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_settings" app:keyIcon="@drawable/sym_keyboard_settings" app:iconPreview="@drawable/sym_keyboard_feedback_settings" app:isModifier="true"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="25%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_url_with_settings_key" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_settings" app:keyIcon="@drawable/sym_keyboard_settings" app:iconPreview="@drawable/sym_keyboard_feedback_settings" app:isModifier="true"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="25%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_email_with_settings_key" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_settings" app:keyIcon="@drawable/sym_keyboard_settings" app:iconPreview="@drawable/sym_keyboard_feedback_settings" app:isModifier="true"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="25%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_im_with_settings_key" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_settings" app:keyIcon="@drawable/sym_keyboard_settings" app:iconPreview="@drawable/sym_keyboard_feedback_settings" app:isModifier="true"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:keyLabel=":-)" app:keyOutputText=":-) " app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_smileys" app:keyWidth="25%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
    <Row app:keyboardMode="@+id/mode_webentry_with_settings_key" app:rowEdgeFlags="bottom">
        <Key app:codes="@integer/key_symbol" app:keyLabel="@string/label_symbol_key" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="left"/>
        <Key app:codes="@integer/key_settings" app:keyIcon="@drawable/sym_keyboard_settings" app:iconPreview="@drawable/sym_keyboard_feedback_settings" app:isModifier="true"/>
        <Key app:codes="@integer/key_f1" app:isModifier="true"/>
        <Key app:codes="@integer/key_space" app:keyIcon="@drawable/sym_keyboard_space" app:iconPreview="@drawable/sym_keyboard_feedback_space" app:keyWidth="30%p" app:isModifier="true"/>
        <Key app:codes="@integer/key_tab" app:keyIcon="@drawable/sym_keyboard_tab" app:iconPreview="@drawable/sym_keyboard_feedback_tab" app:isModifier="true"/>
        <Key app:keyLabel="." app:keyIcon="@drawable/hint_popup" app:popupKeyboard="@xml/popup_punctuation" app:isModifier="true"/>
        <Key app:codes="@integer/key_return" app:keyIcon="@drawable/sym_keyboard_return" app:iconPreview="@drawable/sym_keyboard_feedback_return" app:keyWidth="15%p" app:isModifier="true" app:keyEdgeFlags="right"/>
    </Row>
</Keyboard>