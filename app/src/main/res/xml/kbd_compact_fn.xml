<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:horizontalGap="0px"
    app:keyWidth="10%p"
    app:layoutColumns="11"
    app:layoutRows="5"
    app:verticalGap="0px"
    app:verticalPad="@dimen/key_vertical_pad_compact">
    <Row app:rowEdgeFlags="top">
        <Key
            app:codes="@integer/key_escape"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="Esc"
            app:keyWidth="12%p" />
        <Key
            app:codes="@integer/key_f1"
            app:isModifier="true"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f1"
            app:isModifier="true"
            app:keyLabel="F1"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f2"
            app:isModifier="true"
            app:keyLabel="F2"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f3"
            app:isModifier="true"
            app:keyLabel="F3"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f4"
            app:isModifier="true"
            app:keyLabel="F4"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f5"
            app:isModifier="true"
            app:keyLabel="F5"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f6"
            app:isModifier="true"
            app:keyLabel="F6"
            app:keyWidth="9%p" />

        <Key
            app:codes="@integer/key_break"
            app:isModifier="true"
            app:keyLabel="Brk"
            app:keyWidth="12%p" />

        <Key
            app:codes="@integer/key_delete"
            app:iconPreview="@drawable/sym_keyboard_feedback_delete"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="13%p" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_sysrq"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="SyRq"
            app:keyWidth="12%p" />
        <Key
            app:codes="@integer/key_settings"
            app:iconPreview="@drawable/sym_keyboard_feedback_settings"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_settings"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f7"
            app:isModifier="true"
            app:keyLabel="F7"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f8"
            app:isModifier="true"
            app:keyLabel="F8"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f9"
            app:isModifier="true"
            app:keyLabel="F9"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f10"
            app:isModifier="true"
            app:keyLabel="F10"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f11"
            app:isModifier="true"
            app:keyLabel="F11"
            app:keyWidth="9%p" />
        <Key
            app:codes="@integer/key_fkey_f12"
            app:isModifier="true"
            app:keyLabel="F12"
            app:keyWidth="9%p" />

        <Key
            app:codes="@integer/key_insert"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="Ins"
            app:keyWidth="12%p" />

        <Key
            app:codes="@integer/key_page_up"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyLabel="PgUp"
            app:keyWidth="13%p" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_scroll_lock"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="ScrL"
            app:keyWidth="12%p" />
        <Key
            app:keyLabel="~"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="_"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="+"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="{"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="}"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="|"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel=":"
            app:keyWidth="9%p" />

        <Key
            app:codes="@integer/key_forward_del"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="Del"
            app:keyWidth="12%p" />
        <Key
            app:codes="@integer/key_page_down"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyLabel="PgDn"
            app:keyWidth="13%p" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_num_lock"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="NumL"
            app:keyWidth="12%p" />

        <Key
            app:keyLabel="`"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="-"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="="
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="["
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="]"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel="\\"
            app:keyWidth="9%p" />
        <Key
            app:keyLabel=";"
            app:keyWidth="9%p" />

        <Key
            app:codes="@integer/key_home"
            app:isModifier="true"
            app:keyLabel="Home"
            app:keyWidth="12%p" />
        <Key
            app:codes="@integer/key_end"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyLabel="End"
            app:keyWidth="13%p" />
    </Row>

    <Row
        app:keyWidth="9.09090909%p"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_fn"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="Fn"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_ctrl_left"
            app:isModifier="true"
            app:isSticky="true"
            app:keyLabel="Ctrl"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_alt_left"
            app:isModifier="true"
            app:isSticky="true"
            app:keyLabel="Alt"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_meta_left"
            app:isModifier="true"
            app:isSticky="true"
            app:keyLabel="&#x2756;"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_shift"
            app:iconPreview="@drawable/sym_keyboard_feedback_shift"
            app:isModifier="true"
            app:isSticky="true"
            app:keyIcon="@drawable/sym_keyboard_shift"
            app:keyWidth="8.29090909%p" />
        <Key
            app:keyIcon="@drawable/hint_popup"
            app:keyLabel=".com"
            app:keyOutputText=".com"
            app:keyWidth="10.145454545%p"
            app:popupKeyboard="@xml/popup_domains" />
        <Key
            app:keyIcon="@drawable/hint_popup"
            app:keyLabel=":-)"
            app:keyOutputText=":-) "
            app:keyWidth="10.145454545%p"
            app:popupKeyboard="@xml/popup_smileys" />
        <Key
            app:codes="@integer/key_dpad_center"
            app:isCursor="true"
            app:isModifier="true"
            app:keyLabel="&#x25cb;"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_dpad_left"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25c1;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_down"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25bd;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_up"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25b3;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_right"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyLabel="&#x25b7;"
            app:keyWidth="7.49090909%p" />
    </Row>
</Keyboard>
