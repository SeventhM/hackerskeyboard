<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:horizontalGap="0px"
    app:keyWidth="9.09090909%p"
    app:layoutColumns="11"
    app:layoutRows="5"
    app:verticalGap="0px"
    app:verticalPad="@dimen/key_vertical_pad_compact">
    <Row app:extension="true">
        <Key
            app:codes="@integer/key_escape"
            app:isModifier="true"
            app:keyLabel="Esc" />
        <Key
            app:keyLabel="`"
            app:shiftLabel="~" />
        <Key
            app:keyLabel="-"
            app:shiftLabel="_" />
        <Key
            app:keyLabel="\\"
            app:shiftLabel="|" />
        <Key
            app:keyLabel=";"
            app:shiftLabel=":" />
        <Key
            app:keyLabel="="
            app:shiftLabel="+" />
        <Key
            app:keyLabel="["
            app:shiftLabel="{" />
        <Key
            app:keyLabel="]"
            app:shiftLabel="}" />
        <Key
            app:codes="@integer/key_home"
            app:isModifier="true"
            app:keyLabel="Home" />
        <Key
            app:codes="@integer/key_end"
            app:isModifier="true"
            app:keyLabel="End" />
        <Key
            app:codes="@integer/key_forward_del"
            app:isModifier="true"
            app:keyLabel="Del" />
    </Row>

    <Row>
        <Key
            app:keyEdgeFlags="left"
            app:keyLabel="1"
            app:keyWidth="7.09090909%p"
            app:popupCharacters="`"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="!" />
        <Key
            app:keyLabel="2"
            app:popupCharacters="~"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="\@" />
        <Key
            app:keyLabel="3"
            app:popupCharacters="-"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="#" />
        <Key
            app:keyLabel="4"
            app:popupCharacters="_"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="$" />
        <Key
            app:keyLabel="5"
            app:popupCharacters="="
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="%" />
        <Key
            app:keyLabel="6"
            app:popupCharacters="+"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="^" />
        <Key
            app:keyLabel="7"
            app:popupCharacters="{"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="&amp;" />
        <Key
            app:keyLabel="8"
            app:popupCharacters="}"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="*" />
        <Key
            app:keyLabel="9"
            app:popupCharacters="["
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="(" />
        <Key
            app:keyLabel="0"
            app:popupCharacters="]"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel=")" />
        <Key
            app:codes="@integer/key_delete"
            app:iconPreview="@drawable/sym_keyboard_feedback_delete"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="11.09090909%p" />
    </Row>
    <Row>
        <Key
            app:keyEdgeFlags="left"
            app:keyLabel="q"
            app:popupCharacters="@string/alternates_for_q"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="w"
            app:popupCharacters="@string/alternates_for_w"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="e"
            app:popupCharacters="@string/alternates_for_e"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="r"
            app:popupCharacters="@string/alternates_for_r"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="t"
            app:popupCharacters="@string/alternates_for_t"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="y"
            app:popupCharacters="@string/alternates_for_y"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="u"
            app:popupCharacters="@string/alternates_for_u"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="i"
            app:popupCharacters="@string/alternates_for_i"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="o"
            app:popupCharacters="@string/alternates_for_o"
            app:popupKeyboard="@xml/kbd_popup_narrow_template" />
        <Key
            app:keyLabel="p"
            app:popupCharacters="@string/alternates_for_p"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyEdgeFlags="right"
            app:keyLabel="'"
            app:popupCharacters="|"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="&quot;" />
    </Row>
    <Row>
        <Key
            app:horizontalGap="4.54545454%p"
            app:keyEdgeFlags="left"
            app:keyLabel="a"
            app:popupCharacters="@string/alternates_for_a"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="s"
            app:popupCharacters="@string/alternates_for_s"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="d"
            app:popupCharacters="@string/alternates_for_d"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="f"
            app:popupCharacters="@string/alternates_for_f"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="g"
            app:popupCharacters="@string/alternates_for_g"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="h"
            app:popupCharacters="@string/alternates_for_h"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="j"
            app:popupCharacters="@string/alternates_for_j"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="k"
            app:popupCharacters="@string/alternates_for_k"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="l"
            app:popupCharacters="@string/alternates_for_l"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:codes="@integer/key_return"
            app:iconPreview="@drawable/sym_keyboard_feedback_return"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="13.6363636%p" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_shift"
            app:iconPreview="@drawable/sym_keyboard_feedback_shift"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/sym_keyboard_shift"
            app:keyWidth="11.09090909%p" />
        <Key
            app:keyLabel="z"
            app:popupCharacters="@string/alternates_for_z"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="x"
            app:popupCharacters="@string/alternates_for_x"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="c"
            app:popupCharacters="@string/alternates_for_c"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="v"
            app:popupCharacters="@string/alternates_for_v"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="b"
            app:popupCharacters="@string/alternates_for_b"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="n"
            app:popupCharacters="@string/alternates_for_n"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="m"
            app:popupCharacters="@string/alternates_for_m"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel=","
            app:popupCharacters=";"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="&lt;" />
        <Key
            app:keyIcon="@drawable/hint_popup"
            app:keyLabel="."
            app:popupCharacters=":"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="&gt;" />
        <Key
            app:codes="@integer/key_f1"
            app:keyEdgeFlags="right"
            app:keyLabel="/"
            app:keyWidth="7.09090909%p"
            app:popupCharacters="\\"
            app:popupKeyboard="@xml/kbd_popup_template"
            app:shiftLabel="\?" />
    </Row>
    <Row app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_fn"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="Fn"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_ctrl_left"
            app:isModifier="true"
            app:isSticky="true"
            app:keyLabel="Ctrl"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_alt_left"
            app:isModifier="true"
            app:isSticky="true"
            app:keyLabel="Alt"
            app:keyWidth="8.29090909%p" />
        <Key
            app:codes="@integer/key_tab"
            app:iconPreview="@drawable/sym_keyboard_feedback_tab"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_tab"
            app:keyWidth="8.29090909%p"
            app:popupKeyboard="@xml/popup_tab" />
        <Key
            app:codes="@integer/key_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="36.87272727%p" />
        <Key
            app:codes="@integer/key_dpad_left"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25c1;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_down"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25bd;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_up"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyLabel="&#x25b3;"
            app:keyWidth="7.49090909%p" />
        <Key
            app:codes="@integer/key_dpad_right"
            app:isCursor="true"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyLabel="&#x25b7;"
            app:keyWidth="7.49090909%p" />
    </Row>
</Keyboard>
