<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:horizontalGap="0px"
    app:keyWidth="10%p"
    app:verticalGap="@dimen/key_bottom_gap">
    <Row app:rowEdgeFlags="top">
        <Key
            app:keyEdgeFlags="left"
            app:keyLabel="~" />
        <Key app:keyLabel="`" />
        <Key app:keyLabel="|" />
        <Key
            app:keyLabel="•"
            app:popupCharacters="♪♥♠♦♣"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="√" />
        <Key
            app:keyLabel="π"
            app:popupCharacters="Π"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="÷" />
        <Key app:keyLabel="×" />
        <Key app:keyLabel="{" />
        <Key
            app:keyEdgeFlags="right"
            app:keyLabel="}" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_tab"
            app:keyEdgeFlags="left"
            app:keyLabel="\u21E5" />
        <Key app:keyLabel="£" />
        <Key app:keyLabel="¢" />
        <Key app:keyLabel="€" />
        <Key app:keyLabel="°" />
        <Key
            app:keyLabel="^"
            app:popupCharacters="↑↓←→"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="_" />
        <Key
            app:keyLabel="="
            app:popupCharacters="≠≈∞"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="[" />
        <Key
            app:keyEdgeFlags="right"
            app:keyLabel="]" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_shift"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alt_key"
            app:keyWidth="15%p" />
        <Key app:keyLabel="™" />
        <Key app:keyLabel="®" />
        <Key app:keyLabel="©" />
        <Key
            app:keyLabel="¶"
            app:popupCharacters="§"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="\\" />
        <Key
            app:keyLabel="&lt;"
            app:popupCharacters="≤«‹"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="&gt;"
            app:popupCharacters="≥»›"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:codes="@integer/key_delete"
            app:iconPreview="@drawable/sym_keyboard_feedback_delete"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="15%p" />
    </Row>
    <Row
        app:keyboardMode="@+id/mode_symbols"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_symbol"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alpha_key"
            app:keyWidth="20%p" />
        <Key
            app:isModifier="true"
            app:keyLabel="„" />
        <Key
            app:codes="@integer/key_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="40%p" />
        <Key
            app:isModifier="true"
            app:keyLabel="…" />
        <Key
            app:codes="@integer/key_return"
            app:iconPreview="@drawable/sym_keyboard_feedback_return"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="20%p" />
    </Row>
    <Row
        app:keyboardMode="@+id/mode_symbols_with_settings_key"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_symbol"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alpha_key"
            app:keyWidth="15%p" />
        <Key
            app:codes="@integer/key_settings"
            app:iconPreview="@drawable/sym_keyboard_feedback_settings"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_settings" />
        <Key
            app:isModifier="true"
            app:keyLabel="„" />
        <Key
            app:codes="@integer/key_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="30%p" />
        <Key
            app:isModifier="true"
            app:keyLabel="…" />
        <Key
            app:codes="@integer/key_return"
            app:iconPreview="@drawable/sym_keyboard_feedback_return"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="25%p" />
    </Row>
</Keyboard>
