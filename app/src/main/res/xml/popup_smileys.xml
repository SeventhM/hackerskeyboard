<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="15%p"
    app:horizontalGap="0px"
    app:verticalGap="0px"
    app:keyHeight="@dimen/popup_key_height"
>
    <Row
        app:rowEdgeFlags="top"
    >
        <Key
            app:keyLabel=":-)"
            app:keyOutputText=":-) "
            app:keyEdgeFlags="left" />
        <Key
            app:keyLabel=":-("
            app:keyOutputText=":-( " />
        <Key
            app:keyLabel=";-)"
            app:keyOutputText=";-) " />
        <Key
            app:keyLabel=":-P"
            app:keyOutputText=":-P " />
        <Key
            app:keyLabel="=-O"
            app:keyOutputText="=-O "
            app:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            app:keyLabel=":-*"
            app:keyOutputText=":-* "
            app:keyEdgeFlags="left" />
        <Key
            app:keyLabel=":O"
            app:keyOutputText=":O " />
        <Key
            app:keyLabel="B-)"
            app:keyOutputText="B-) " />
        <Key
            app:keyLabel=":-$"
            app:keyOutputText=":-$ " />
        <Key
            app:keyLabel=":-!"
            app:keyOutputText=":-! "
            app:keyEdgeFlags="right" />
    </Row>
    <Row
        app:rowEdgeFlags="bottom"
    >
        <Key
            app:keyLabel=":-["
            app:keyOutputText=":-[ "
            app:keyEdgeFlags="left" />
        <Key
            app:keyLabel="O:-)"
            app:keyOutputText="O:-) " />
        <Key
            app:keyLabel=":-\\"
            app:keyOutputText=":-\\ " />
        <Key
            app:keyLabel=":'("
            app:keyOutputText=":'( " />
        <Key
            app:keyLabel=":-D"
            app:keyOutputText=":-D "
            app:keyEdgeFlags="right" />
    </Row>
</Keyboard>
