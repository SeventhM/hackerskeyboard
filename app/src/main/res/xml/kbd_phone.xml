<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="26.67%p"
    app:horizontalGap="0px"
    app:verticalGap="@dimen/key_bottom_gap"
>
    <Row
        app:rowEdgeFlags="top"
    >
        <Key
            app:codes="49"
            app:keyIcon="@drawable/sym_keyboard_num1"
            app:keyEdgeFlags="left" />
        <Key
            app:codes="50"
            app:keyIcon="@drawable/sym_keyboard_num2" />
        <Key
            app:codes="51"
            app:keyIcon="@drawable/sym_keyboard_num3" />
        <Key
            app:keyLabel="-"
            app:keyWidth="20%p"
            app:isModifier="true"
            app:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            app:codes="52"
            app:keyIcon="@drawable/sym_keyboard_num4"
            app:keyEdgeFlags="left" />
        <Key
            app:codes="53"
            app:keyIcon="@drawable/sym_keyboard_num5" />
        <Key
            app:codes="54"
            app:keyIcon="@drawable/sym_keyboard_num6" />
        <Key
            app:keyLabel="."
            app:keyWidth="20%p"
            app:isModifier="true"
            app:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            app:codes="55"
            app:keyIcon="@drawable/sym_keyboard_num7"
            app:keyEdgeFlags="left" />
        <Key
            app:codes="56"
            app:keyIcon="@drawable/sym_keyboard_num8" />
        <Key
            app:codes="57"
            app:keyIcon="@drawable/sym_keyboard_num9" />
        <Key
            app:codes="@integer/key_delete"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:iconPreview="@drawable/sym_keyboard_feedback_delete"
            app:keyWidth="20%p"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right" />
    </Row>
    <Row
        app:rowEdgeFlags="bottom"
    >
        <Key
            app:codes="@integer/key_symbol"
            app:keyIcon="@drawable/sym_keyboard_numalt"
            app:iconPreview="@drawable/sym_keyboard_feedback_numalt"
            app:keyEdgeFlags="left" />
        <Key
            app:codes="48"
            app:keyIcon="@drawable/sym_keyboard_num0" />
        <Key
            app:codes="@integer/key_space"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space" />
        <Key
            app:codes="@integer/key_return"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="20%p"
            app:isModifier="true"
            app:keyEdgeFlags="right" />
    </Row>
</Keyboard>
