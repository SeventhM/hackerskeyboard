<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:horizontalGap="0px"
    app:keyWidth="10%p"
    app:verticalGap="@dimen/key_bottom_gap">
    <Row app:rowEdgeFlags="top">
        <Key
            app:keyEdgeFlags="left"
            app:keyLabel="1"
            app:popupCharacters="¹½⅓¼⅛"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="2"
            app:popupCharacters="²⅔"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="3"
            app:popupCharacters="³¾⅜"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="4"
            app:popupCharacters="⁴"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="5"
            app:popupCharacters="⅝"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="6" />
        <Key
            app:keyLabel="7"
            app:popupCharacters="⅞"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="8" />
        <Key app:keyLabel="9" />
        <Key
            app:keyEdgeFlags="right"
            app:keyLabel="0"
            app:popupCharacters="ⁿ∅"
            app:popupKeyboard="@xml/kbd_popup_template" />
    </Row>
    <Row>
        <Key
            app:keyEdgeFlags="left"
            app:keyLabel="\@" />
        <Key app:keyLabel="\#" />
        <Key
            app:keyLabel="$"
            app:popupCharacters="¢£€¥₣₤₱"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="%"
            app:popupCharacters="‰"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel="&amp;" />
        <Key
            app:keyLabel="*"
            app:popupCharacters="†‡★"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="-"
            app:popupCharacters="_–—"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="+"
            app:popupCharacters="±"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="("
            app:popupCharacters="[{&lt;"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyEdgeFlags="right"
            app:keyLabel=")"
            app:popupCharacters="]}&gt;"
            app:popupKeyboard="@xml/kbd_popup_template" />
    </Row>
    <Row>
        <Key
            app:codes="@integer/key_shift"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alt_key"
            app:keyWidth="15%p" />
        <Key
            app:keyLabel="!"
            app:popupCharacters="¡"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="&quot;"
            app:popupCharacters="“”«»˝"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:keyLabel="\'"
            app:popupCharacters="‘’"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key app:keyLabel=":" />
        <Key app:keyLabel=";" />
        <Key app:keyLabel="/" />
        <Key
            app:keyLabel="\?"
            app:popupCharacters="¿"
            app:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            app:codes="@integer/key_delete"
            app:iconPreview="@drawable/sym_keyboard_feedback_delete"
            app:isModifier="true"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="15%p" />
    </Row>
    <Row
        app:keyboardMode="@+id/mode_symbols"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_symbol"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alpha_key"
            app:keyWidth="20%p" />
        <Key
            app:codes="@integer/key_f1"
            app:isModifier="true" />
        <Key
            app:codes="@integer/key_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="40%p" />
        <Key
            app:isModifier="true"
            app:keyIcon="@drawable/hint_popup"
            app:keyLabel="."
            app:popupKeyboard="@xml/popup_punctuation" />
        <Key
            app:codes="@integer/key_return"
            app:iconPreview="@drawable/sym_keyboard_feedback_return"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="20%p" />
    </Row>
    <Row
        app:keyboardMode="@+id/mode_symbols_with_settings_key"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="@integer/key_symbol"
            app:isModifier="true"
            app:keyEdgeFlags="left"
            app:keyLabel="@string/label_alpha_key"
            app:keyWidth="15%p" />
        <Key
            app:codes="@integer/key_settings"
            app:iconPreview="@drawable/sym_keyboard_feedback_settings"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_settings" />
        <Key
            app:codes="@integer/key_f1"
            app:isModifier="true" />
        <Key
            app:codes="@integer/key_space"
            app:iconPreview="@drawable/sym_keyboard_feedback_space"
            app:isModifier="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="30%p" />
        <Key
            app:isModifier="true"
            app:keyIcon="@drawable/hint_popup"
            app:keyLabel="."
            app:popupKeyboard="@xml/popup_punctuation" />
        <Key
            app:codes="@integer/key_return"
            app:iconPreview="@drawable/sym_keyboard_feedback_return"
            app:isModifier="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="25%p" />
    </Row>
</Keyboard>
